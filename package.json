    "escape-html": "^1.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "esno": "^0.5.0",
    "glob": "^7.1.7",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "jpeg-js": "^0.4.3",
    "klaw-sync": "^6.0.0",
    "lint-staged": "10",
    "markdown-it": "^13.0.1",
