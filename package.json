{
  "name": "pandora-lib",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite --host",
    "serve": "vite preview",
    "build:bundle": "pnpm gen-entry && cross-env NODE_ENV=build rollup -c ./scripts/rollup.config.ts",
    "build:lib": "esno ./scripts/esbuild.ts",
    "build": "rimraf dist && esno ./scripts/gen-dts.ts && pnpm build:bundle && pnpm build:lib",
    "build:example": "vue-tsc --noEmit && vite build --base=./",
    "docs:dev": "esno scripts/gen-docs.ts && esno ./scripts/dev-docs.ts",
    "docs:build": "esno scripts/gen-docs.ts && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .eslintrc.js . --fix",
    "gen": "esno ./scripts/gen-component.ts --name",
    "gen-entry": "esno ./scripts/gen-entry.ts",
    "test": "jest --coverage",
    "commit": "git-cz",
    "release": "esno ./scripts/release.ts",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.16.1"
  },
  "files": [
    "dist"
  ],
  "main": "dist/lib/pandora-lib.umd.js",
  "module": "dist/es/pandora-lib.esm.js",
  "typings": "dist/types/pandora-lib.d.ts",
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@iconify/json": "^1.1.368",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/echarts": "^4.9.18",
    "@types/escape-html": "^1.0.2",
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/js-cookie": "^3.0.3",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash-es": "^4.17.7",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/minimist": "^1.2.2",
    "@types/node": "^15.0.2",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@types/stylus": "^0.48.34",
    "@typescript-eslint/eslint-plugin": "5.60",
    "@typescript-eslint/parser": "5.60",
    "@unocss/preset-wind": "^0.35.2",
    "@vitejs/plugin-vue": "^1.9.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/runtime-core": "^3.2.36",
    "@vue/test-utils": "^2.0.0-rc.8",
    "@vue/vue3-jest": "^27.0.0",
    "@vueuse/core": "8.2",
    "@vueuse/motion": "^1.5.6",
    "autoprefixer": "^10.2.5",
