    "vue-tsc": "1.8.1"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@unocss/reset": "^0.35.2",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.7",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
